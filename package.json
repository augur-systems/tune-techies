{
  "name": "tune-techies",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build:worklets": "webpack --config webpack.worklets.config.js",
    "build": "next build && npm run build:worklets",
    "start": "npm run build && next start",
    "lint": "next lint",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown ."
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.1.9",
    "aws-amplify": "^6.2.0",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.0.0",
    "@aws-amplify/backend-cli": "^1.0.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/audioworklet": "^0.0.55",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "glob": "^10.4.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
